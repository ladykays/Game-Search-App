<% if (games && games.length > 0) { %>
  <div class="flex flex-col gap-8">
    <% games.forEach((game) => { %>
      <a class="flex flex-col " href="/game/<%= game.id %>">
        <h3 class="text-lg font-normal text-gray-100 line-clamp-1">
          <%= game.name %>
        </h3>
        <img
          src="<%= game.background_image %>"
          alt="<%= game.name %>"
          class=""
        />
        
      </a>
    <% }); %>
  </div>
<% } %>